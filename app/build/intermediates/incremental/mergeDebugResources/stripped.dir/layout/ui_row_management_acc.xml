<?xml version="1.0" encoding="utf-8"?>

                                                              
                                                        
    
                                          
                                                                                                       
           

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?selectableItemBackground"
        android:id="@+id/managementParkingSpot"
        android:minHeight="72dp" android:tag="layout/ui_row_management_acc_0" xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">


        <TextView
            android:id="@+id/keyIDManageAccount"
            style="@style/ListItemPrimary"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:ellipsize="end"
            android:singleLine="true"
            android:tag="binding_1"                     
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewKeyIDManagementAcc"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fieldNameManageAccount"
            style="@style/ListItemSecondary"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="16dp"
            android:ellipsize="end"
            android:singleLine="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewFieldNameManagementAcc"
            app:layout_constraintTop_toBottomOf="@+id/keyIDManageAccount"
            android:tag="binding_2"                      />

        <TextView
            android:id="@+id/textViewKeyIDManagementAcc"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="26dp"
            android:text="@string/keyID"
            android:textSize="16sp"
            app:layout_constraintEnd_toStartOf="@+id/keyIDManageAccount"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewFieldNameManagementAcc"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:text="@string/fieldName"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewKeyIDManagementAcc" />

    </androidx.constraintlayout.widget.ConstraintLayout>

         