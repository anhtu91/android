<?xml version="1.0" encoding="utf-8"?>

                                                              
                                                        
    
                                          
                                                                                       
           

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?selectableItemBackground"
        android:id="@+id/importQRCodeRow"
        android:minHeight="72dp" android:tag="layout/ui_row_parkplatz_0" xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">


        <TextView
            android:id="@+id/keyIDParkplatz"
            style="@style/ListItemPrimary"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="5dp"
            android:ellipsize="end"
            android:singleLine="true"
            android:tag="binding_1"             
            app:layout_constraintEnd_toStartOf="@+id/dateTimeParkplatz"
            app:layout_constraintStart_toEndOf="@+id/textViewKeyIDParkplatz"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/dateTimeParkplatz"
            style="@style/ListItemSecondary"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:gravity="end"
            android:singleLine="true"
            android:tag="binding_2"                
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/keyIDParkplatz"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fieldNameParkplatz"
            style="@style/ListItemSecondary"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="16dp"
            android:ellipsize="end"
            android:singleLine="true"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textViewFieldNameParkplatz"
            app:layout_constraintTop_toBottomOf="@+id/keyIDParkplatz"
            android:tag="binding_3"              />

        <TextView
            android:id="@+id/textViewKeyIDParkplatz"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/keyID"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewFieldNameParkplatz"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:text="@string/fieldName"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewKeyIDParkplatz" />

    </androidx.constraintlayout.widget.ConstraintLayout>

         